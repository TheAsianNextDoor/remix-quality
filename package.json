{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "NODE_ENV=production run-s \"build:*\"",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "check-updates": "ncu",
    "clean": "rm -rf ./node_modules & rm ./package-lock.json",
    "coverage": "NODE_ENV=test node_modules/.bin/vitest run --coverage",
    "coverage:view": "NODE_ENV=test npm run coverage && open ./reports/index.html",
    "db:gui": "prisma studio",
    "db:pull": "prisma pull",
    "db:push": "prisma db push",
    "db:seed": "prisma db seed",
    "db:types": "prisma generate",
    "db:update": "prisma pull && prisma push && prisma generate",
    "dev": "run-p \"dev:*\"",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "remix dev",
    "generate:css": "tailwindcss -o ./app/tailwind.css",
    "lint": "eslint './src'",
    "lint:fix": "concurrently -n prettier,eslint \"prettier --write . '!**/*.{js,jsx,ts,tsx}'\" \"eslint --fix .\"",
    "playwright:test": "playwright test --project=chromium --project=firefox",
    "playwright:debug-test": "playwright test --project=chromium --debug --",
    "start": "cross-env NODE_ENV=production netlify dev",
    "test": "NODE_ENV=test vitest run",
    "test:watch": "NODE_ENV=test vitest watch",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/server": "^11.10.0",
    "@mantine/core": "^5.8.0",
    "@mantine/hooks": "^5.8.0",
    "@mantine/remix": "^5.8.0",
    "@netlify/functions": "^1.3.0",
    "@prisma/client": "^4.6.1",
    "@remix-run/netlify": "^1.7.5",
    "@remix-run/node": "^1.7.5",
    "@remix-run/react": "^1.7.5",
    "@tabler/icons": "^1.111.0",
    "cross-env": "^7.0.3",
    "prisma": "^4.6.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@emotion/babel-plugin": "^11.10.5",
    "@playwright/test": "^1.27.1",
    "@remix-run/dev": "^1.7.5",
    "@remix-run/eslint-config": "^1.7.5",
    "@remix-run/serve": "^1.7.5",
    "@testing-library/dom": "^8.18.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/eslint": "^8.4.6",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "@vitejs/plugin-react": "^2.1.0",
    "@vitest/coverage-c8": "^0.23.4",
    "@vitest/ui": "^0.25.1",
    "autoprefixer": "^10.4.13",
    "c8": "^7.12.0",
    "concurrently": "^7.5.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.27.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vitest-globals": "^1.2.0",
    "happy-dom": "^6.0.4",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.3",
    "msw": "^0.47.3",
    "npm-check-updates": "^16.3.22",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.19",
